{
  "name": "typestruct",
  "version": "1.0.0",
  "description": "Typescript Structure Abilities",
  "scripts": {
    "clean": "rimraf _bundles lib lib-esm dist",
    "build": "webpack-dev-server -w --hot --inline --config webpack.config.js",
    "prod": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --config webpack.production.config.js",
    "start": "gulp",
    "test": "gulp karma:test"
  },
